{
  "name": "ionic",
  "version": "1.1.1",
  "description": "ionic: An Ionic project",
  "dependencies": {
    "angular-route": "^1.5.7",
    "gulp": "^3.5.6",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.0.4",
    "ionic": "^1.2.4"
  },
  "devDependencies": {
    "bower": "^1.3.3",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "gulp-jshint": "^2.0.1",
    "gulp-util": "^2.2.14",
    "jshint": "^2.9.2",
    "shelljs": "^0.3.0",
    "vinyl-source-stream": "^1.1.0"
